{
  "name": "addWordHelper",
  "manifest_version": 2,
  "version": "1.0",
  "description": "Help you prepare for GRE",
  "permissions": [
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "img/logo.png"
  },
  "content_scripts": [
    {
      "matches": ["http://gre.kmf.com/practise/result/*"],
      "js": ["script.js",
              "jquery-1.9.1.js"
            ]
    }
  ],
  "icons": {
    "16": "img/16.png",
    "128": "img/128.png"
  }
}